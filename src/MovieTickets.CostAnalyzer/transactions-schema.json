{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "JSON Schema for my JSON file format",

  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "TransactionId": {
          "type": "integer"
        },
        "Customers": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "Name": {
                  "type": "string"
                },
                "Age": {
                  "type": "integer"
                }
              },
              "required": [
                "Name",
                "Age"
              ]
            }
          ]
        }
      },
      "required": [
        "TransactionId",
        "Customers"
      ]
    }
  ]
}